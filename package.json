{
  "name": "@gurja/eslint-config",
  "version": "6.0.1",
  "description": "A simple to use ESlint configuration",
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "typescript",
    "react",
    "next",
    "node"
  ],
  "homepage": "https://github.com/leogurja/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/leogurja/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leogurja/eslint-config.git"
  },
  "license": "MIT",
  "author": "Leonardo Gurgel",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./jest": "./dist/jest.js",
    "./next": "./dist/next.js",
    "./node": "./dist/node.js",
    "./react": "./dist/react.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "@types"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier -w .",
    "inspect": "eslint --inspect-config",
    "prelint": "pnpm build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "dependencies": {
    "@eslint/js": "^9.37.0",
    "@humanwhocodes/gitignore-to-minimatch": "^1.0.2",
    "@next/eslint-plugin-next": "^15.5.4",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^60.8.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^6.1.1",
    "find-up": "^8.0.0",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.3.0",
    "@microsoft/api-extractor": "^7.53.0",
    "@types/node": "^24.7.0",
    "eslint": "^9.37.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "ts-jest": "^29.4.4",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "typescript": "^5.8.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  }
}
